<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/baseline_desktop_windows_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_desktop_windows_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.Laboratorio4"
        tools:targetApi="31">
        <activity
            android:name=".MainNavBar"
            android:exported="false"
            android:label="@string/title_activity_main_nav_bar"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".EmergencyContacts"
            android:exported="false"
            android:label="@string/title_activity_emergency_contacts"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".Settings"
            android:exported="false"
            android:label="@string/title_activity_settings"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".MyProfile"
            android:exported="false"
            android:label="@string/title_activity_my_profile"
            android:theme="@style/Theme.Laboratorio4" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Laboratorio4">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>